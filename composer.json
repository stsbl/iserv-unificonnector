{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "art-of-wifi/unifi-api-client": "^1.1",
    "iserv/config": "^1.1",
    "iserv/database": "^1.0",
    "symfony/console": "5.4.*",
    "symfony/dotenv": "5.4.*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "5.4.*",
    "symfony/runtime": "5.4.*",
    "symfony/yaml": "5.4.*"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "autoload": {
    "psr-4": {
      "Stsbl\\IServ\\Module\\UnifiConnector\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Stsbl\\IServ\\Module\\UnifiConnector\\Tests\\Unit\\": "tests/Unit/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.iserv.eu/"
    }
  ],
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.4.*"
    },
    "cotor": {
      "extensions": {
        "phpstan/phpstan": {
          "phpstan/extension-installer": "*",
          "phpstan/phpstan-symfony": "*"
        },
        "vimeo/psalm": {
          "psalm/plugin-symfony": "^3.1"
        }
      },
      "friendsofphp/php-cs-fixer": "*",
      "phpstan/phpstan": "*",
      "phpunit/phpunit": "*"
    }
  }
}
